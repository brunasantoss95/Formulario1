@{
}

<form action="/Pessoa/ListaPessoa" method="POST">
    <fieldset>
        <legend>Pessoa</legend>
        <div>
            <label for="id">Id Pessoa</label>
        </div>
        <div>
            <input type="number" value="@Model.id" id="id" name="id" />
        </div>


        <div>
            <label for="nomepessoa">Nome Pessoa</label>
        </div>
        <div>
            <input type="string" value="@Model.nomepessoa" id="nomepessoa" name="nomepessoa" />
        </div>


        <div>
            <label for="email">email</label>
        </div>
        <div>
            <input type="string" value="@Model.email" id="email" name="email" />
        </div>

        <div>
            <label for="vlr_renda">vlr_renda</label>
        </div>
        <div>
            <input type="double" value="@Model.vlr_renda" id="vlr_renda" name="vlr_renda" />
        </div>

        <p><input type="submit" value="enviar" /></p>
    </fieldset>

</form>
